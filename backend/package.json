{
  "name": "ticket-system-backend",
  "version": "1.0.0",
  "description": "Backend API for the ticket system",
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "prisma generate",
    "prebuild": "prisma generate",
    "predev": "npm run db:setup",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/scripts/dev-server.ts",
    "build": "tsc",
    "start": "node -r module-alias/register -e \"require('./dist/alias.js'); require('./dist/server.js')\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "ts-node src/database/seed.ts",
    "db:seed:demo": "ts-node src/scripts/seed-demo.ts",
    "db:setup": "ts-node src/scripts/setup-database.ts",
    "db:setup:reset": "ts-node src/scripts/setup-database.ts --reset",
    "db:setup:no-seed": "ts-node src/scripts/setup-database.ts --no-seed",
    "db:setup:simple": "ts-node src/scripts/simple-setup.ts",
    "db:setup:simple:seed": "ts-node src/scripts/simple-setup.ts --seed",
    "db:validate": "prisma validate",
    "db:format": "prisma format",
    "db:studio": "prisma studio",
    "db:backup": "ts-node -e \"import('./src/database/utils').then(m => m.DatabaseUtils.createBackup('./backup.sql'))\"",
    "db:health": "ts-node -e \"import('./src/database/utils').then(m => m.DatabaseUtils.healthCheck().then(console.log))\"",
    "db:stats": "ts-node -e \"import('./src/database/utils').then(m => m.DatabaseUtils.getStats().then(console.log))\"",
    "migration:create": "ts-node src/database/migrations.ts create",
    "migration:run": "ts-node src/database/migrations.ts migrate",
    "migration:init": "ts-node src/database/migrations.ts init"
  },
  "keywords": [
    "ticket-system",
    "api",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mysql2": "^3.14.3",
    "prisma": "^5.7.1",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
